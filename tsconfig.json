{
    "compilerOptions": {
        "target": "ESNext",
        "module": "CommonJS",
        "sourceMap": true,
        "outDir": "dist",
        "resolveJsonModule": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "strictNullChecks": true
        // "strict": true
    },
    "include": [
        "src/**/*",
        "util/InteractiveSetup.ts"
    ],
    "exclude": [
        "node_modules",
        "**/*.spec.ts"
    ]
}
